<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="delsol_financial_state_row_tree" model="ir.ui.view">
			<field name="name">delsol.financial_state_row_config.tree</field>
			<field name="model">delsol.financial_state_row</field>
			<field name="arch" type="xml">
				<tree string="Estados Financieros">
						<field name="name" />
						<field name="description" />
						<field name="code" />
						<field name="excel_sheet" />
						<field name="excel_col" />
						<field name="excel_row" />
						<field name="calculation_formula" />
						<field name="register_type" />
<!-- 								<field name="currency_id" options="{'no_create_edit': True}" /> -->
<!-- 								<field name="vehicle_model_id" options="{'no_create_edit': True}" /> -->
				</tree>
			</field>
		</record>
		<record id="delsol_financial_state_row_form" model="ir.ui.view">
			<field name="name">delsol.financial_state_row_config.form</field>
			<field name="model">delsol.financial_state_row</field>
			<field name="arch" type="xml">
				<form string="Estados Financieros">
					<sheet string="Estados Financieros">
						<group>
							<group>
								<field name="name" />
								<field name="description" />
								<field name="code" />
								<field name="excel_sheet" />
								<field name="excel_col" />
								<field name="excel_row" />
								<field name="calculation_formula" />
								<field name="register_type" />
<!-- 								<field name="currency_id" options="{'no_create_edit': True}" /> -->
<!-- 								<field name="vehicle_model_id" options="{'no_create_edit': True}" /> -->
							</group>
						</group>
					</sheet>
				</form>
			</field>
		</record>



		<record id="open_view_delsol_financial_state_config" model="ir.actions.act_window">
			<field name="name">Configuracion de fila</field>
			<field name="res_model">delsol.financial_state_row</field>
			<field name="view_type">form</field>
			<field name="domain">[]</field>
			<field name="view_mode">tree,form</field>
		</record>


	</data>
</openerp>