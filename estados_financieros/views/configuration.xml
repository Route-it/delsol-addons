<?xml version="1.0" encoding="UTF-8"?>
<openerp>
	<data>

		<record id="est_fin_config_tree" model="ir.ui.view">
			<field name="name">delsol.config.tree</field>
			<field name="model">delsol.config</field>
			<field name="arch" type="xml">
				<tree string="Configuraciones" default_order="create_date desc" editable="bottom" create="0" edit="1">
					<field name="name" readonly="1" />
					<field name="description"  readonly="1"/>
					<field name="value" />
				</tree>
			</field>
		</record>

		<record id="open_view_delsol_parameters_est_fin" model="ir.actions.act_window">
			<field name="name">Configuracion</field>
			<field name="res_model">delsol.config</field>
			<field name="view_type">form</field>
			<field name="view_id" ref="est_fin_config_tree"/>			
			<field name="domain">[('app','=','estados_financieros')]</field>
			<field name="view_mode">tree</field>
		</record>
	</data>
</openerp>